{
  "extends": ["config:base", ":disableRateLimiting"],
  "rangeStrategy": "bump",
  "packageRules": [
    {
      "depTypeList": ["peerDependencies"],
      "rangeStrategy": "widen"
    },
    {
      "depTypeList": ["devDependencies"],
      "excludePackageNames": ["jest", "jest-snapshot"],
      "groupName": "dev dependencies"
    },
    {
      "packageNames": ["jest", "jest-snapshot"],
      "groupName": "jest"
    }
  ]
}
